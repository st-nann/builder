    "uuidv4": "^6.2.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-highlight.js": "^3.1.0",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2"
