{
  "name": "vue-builder-plugin",
  "version": "1.0.180",
  "description": "Builder Template (Vue + Typescript)",
  "private": false,
  "sideEffects": false,
  "main": "dist/vue-builder-plugin.common.js",
  "types": "dist/vue-builder-plugin.d.ts",
  "module": "dist/vue-builder-plugin.esm.js",
  "unpkg": "dist/vue-builder-plugin.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "dev:env": "./setup-env.sh .env.development && vue-cli-service serve --open",
