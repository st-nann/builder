    "build:lib": "vue-cli-service build --target lib --name vue-builder-plugin ./src/plugins/Index.ts",
    "build:ts": "npx cross-env NODE_ENV=development tsc --build",
    "build:tsclean": "tsc --build --clean",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
